<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>User Role</th>
      <th>Channel Permissions</th>
    </tr>
  </thead>
  <tbody>
    <% for (let i = 0; i < users.length; i++) { %>
      <tr>
        <td><%= users[i].email %></td>
        <td><%= users[i].role %></td>
        <td>
          <% for (let j = 0; j < users[i].permissions.length; j++) { %>
            <%= users[i].permissions[j].channel %>:
            <%= users[i].permissions[j].permission %><br>
          <% } %>
        </td>
      </tr>
    <% } %>
  </tbody>
</table>